							state:ModifyStat("Health", RANDOM:NextInteger(-30, -10))
						end
						state.Money = math.max(0, (state.Money or 0) - RANDOM:NextInteger(5000, 15000))
						if state.AddFeed then
							state:AddFeed("ğŸ’¥ CRASH! You wrecked your car and got hurt!")
						end
					end
				end,
			},
