		weight = 25,
		minAge = 18, maxAge = 22,
		baseChance = 0.4,
		oneTime = true,
		eligibility = function(state)
			if not isEligibleForNBA(state) then return false end
			if state.Flags and state.Flags.nba_declared then return false end
