			return hasBasketballBackground(state) or (state.Health or 0) >= 85
		end,
		blockedByFlags = { in_prison = true, injured = true, nba_declared = true },
		
		choices = {
			{
				text = "ğŸ€ YES! I'm ready for the NBA!",
